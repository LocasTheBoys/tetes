print("Test3")
-- Dapatkan karakter pemain
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Pastikan karakter sudah sepenuhnya dimuat
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- Tentukan beberapa titik tujuan
local positions = {
    Vector3.new(-667.29, 231.88, 9.23), -- Tujuan 1
    Vector3.new(-668.6, 243.88, 177.15), -- Tujuan 2
    Vector3.new(-667.16, 255.62, 221.68), -- Tujuan 2
    Vector3.new(-612.05, 271.38, 214.56)  -- Tujuan 3
}

-- Fungsi untuk membuat karakter berjalan menuju titik tertentu
function walkToPosition(targetPosition)
    -- Arahkan Humanoid untuk berjalan ke titik tujuan
    humanoid:MoveTo(targetPosition)

    -- Tunggu sampai karakter sampai di titik tersebut
    humanoid.MoveToFinished:Wait()
end

-- Fungsi untuk berjalan ke semua tujuan
function walkToAllPositions(positions)
    for _, position in ipairs(positions) do
        walkToPosition(position)
    end
end

-- Panggil fungsi untuk berjalan ke semua tujuan
walkToAllPositions(positions)
