print("Test test")
-- Script di bawah ini harus dimasukkan ke dalam LocalScript

-- Dapatkan karakter pemain
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Dapatkan service pathfinding
local PathfindingService = game:GetService("PathfindingService")

-- Titik awal (lokasi karakter saat ini)
local startPosition = character.HumanoidRootPart.Position

-- Tentukan titik kedua (lokasi tujuan)
local endPosition = Vector3.new(-612.05, 271.38, 214.56) -- Gantilah angka ini sesuai kebutuhanmu

-- Dapatkan Humanoid untuk mengendalikan karakter berjalan
local humanoid = character:WaitForChild("Humanoid")
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Fungsi untuk membuat karakter berjalan menuju titik kedua
function walkToPosition(targetPosition)
    -- Buat jalur menggunakan PathfindingService
    local path = PathfindingService:CreatePath({
        AgentRadius = 2, -- Ukuran lebar karakter (disesuaikan)
        AgentHeight = 5, -- Tinggi karakter (disesuaikan)
        AgentCanJump = true, -- Karakter bisa melompat
        AgentJumpHeight = 10, -- Tinggi lompatan karakter
        AgentMaxSlope = 45, -- Kemiringan maksimum yang bisa dilewati
    })

    -- Hitung jalur dari posisi saat ini ke target
    path:ComputeAsync(humanoidRootPart.Position, targetPosition)

    -- Jika jalur valid, maka karakter mulai berjalan mengikuti jalur
    if path.Status == Enum.PathStatus.Complete then
        local waypoints = path:GetWaypoints()

        for _, waypoint in pairs(waypoints) do
            -- Berjalan ke setiap titik di jalur
            humanoid:MoveTo(waypoint.Position)

            -- Tunggu sampai sampai di titik tersebut
            humanoid.MoveToFinished:Wait()

            -- Jika ada waypoint yang memerlukan lompatan
            if waypoint.Action == Enum.PathWaypointAction.Jump then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    else
        -- Jika jalur tidak dapat ditemukan
        warn("Tidak bisa menemukan jalur ke tujuan!")
    end
end

-- Panggil fungsi untuk berjalan dari titik awal ke titik kedua
walkToPosition(endPosition)

